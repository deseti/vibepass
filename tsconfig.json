{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "noImplicitAny": false
  },
  "include": ["./scripts", "./test", "./typechain-types", "./hardhat.config.ts", "./services"],
  "exclude": ["node_modules"]
}
